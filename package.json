{
  "name": "smart-beginning-of-line",
  "displayName": "Smart Beginning of Line",
  "description": "Move cursor to the content beginning and line start.",
  "publisher": "yeannylam",
  "version": "0.5.1",
  "engines": {
    "vscode": "^1.34.0"
  },
  "categories": [
    "Other"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/zhangkaiyulw/smart-beginning-of-line.git"
  },
  "activationEvents": [
    "onCommand:smartBeginningOfLine"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "smartBeginningOfLine",
        "title": "Smart Beginning of Line"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run compile && node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "typescript": "^3.3.1",
    "vscode": "^1.1.28",
    "tslint": "^5.12.1",
    "@types/node": "^10.12.21",
    "@types/mocha": "^2.2.42"
  }
}
